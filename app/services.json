---
limit: 100
---
[
{% for service in site.services limit: page.limit %}
    {
      "layout": "{{ service.layout }}",
      "identifier": "{{ service.identifier }}",
      "sector": "{{ service.sector }}",
      "subsector": "{{ service.subsector }}",
      "title": "{{ service.title }}",
      "preview": "{{ service.preview }}",
      "lang": "{{ service.lang }}",
      "url": "{{ service.url }}"
    }
    {% unless forloop.last %},{% endunless %}
{% endfor %}
]